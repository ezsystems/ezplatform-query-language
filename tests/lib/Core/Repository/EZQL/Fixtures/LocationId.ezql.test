SELECT CONTENT FILTER BY LOCATION ID :locationId
--EXPECTED OUTPUT--
<?php

namespace App\QueryType;

declare(strict_types=1);

use eZ\Publish\API\Repository\Values\Content\Query\Criterion\LocationId;
use eZ\Publish\API\Repository\Values\Content\Query;

class ExampleQuery implements \eZ\Publish\Core\QueryType\QueryType
{

    public function getQuery(array $parameters = []) : \eZ\Publish\API\Repository\Values\Content\Query
    {
        $query = new Query;
        $query->filter = new LocationId(
        	$parameters["locationId"]
        );

        return $query;
    }

    public function getName() : string
    {
        return "ExampleQuery";
    }

    public function getSupportedParameters() : array
    {
        return ["locationId"];
    }


}

